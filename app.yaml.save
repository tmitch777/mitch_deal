cloudbuild.yaml
steps:
  # Install dependencies
  - name: 'python:3.8'
    entrypoint: 'pip'
    args: ['install', '-r', 'requirements.txt']

  # Build and deploy the Flask app
  - name: 'gcr.io/cloud-builders/gcloud'
    entrypoint: 'bash'
    args:
      - '-c'
      - |
        gcloud app deploy app.yaml

